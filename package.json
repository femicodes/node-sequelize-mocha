{
  "name": "sequelize-ci",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "node build/index",
    "clean": "rimraf build",
    "migrate": "sequelize db:migrate",
    "build": "yarn run clean && babel api -d build",
    "dev": "DEBUG=express nodemon --exec babel-node api/index",
    "test": "NODE_ENV=test sequelize db:migrate:undo:all  && NODE_ENV=test sequelize db:migrate  && NODE_ENV=test nyc mocha ./api/test/* --timeout 20000 -r @babel/register -r @babel/polyfill --exit"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.4",
    "@babel/node": "^7.5.0",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.5.4",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-plugin-import": "^2.18.2",
    "mocha": "^6.2.0",
    "nodemon": "^1.19.1",
    "nyc": "^14.1.1",
    "rimraf": "^2.6.3"
  },
  "dependencies": {
    "@babel/register": "^7.4.4",
    "body-parser": "^1.19.0",
    "debug": "^4.1.1",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "morgan": "^1.9.1",
    "pg": "^7.12.0",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.12.2"
  }
}